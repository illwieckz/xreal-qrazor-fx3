// ====================
//  Notes
// ====================
//
// textures/tig    <- diffuse textures ONLY folder
// textures/tig_x  <- normal, specular, alpha, etc folder
// textures/tig_ed <- editor only images folder, often a small jpg of the diffuse
// tig_blar_n8     <- a normalmap with scale 8
// tig_blar_s      <- a specular texture
// tig_blar_sg     <- a grey scale specular
// tig_blar_a      <- an alpha layer
//

textures/shaderlab_wall/cement_drk
{
	qer_editorimage		textures/shaderlab_wall/cement_drk.jpg

	{
		stage	bumpmap
		map	textures/shaderlab_wall/cement_n8.png
	}
	{
		stage	diffusemap
		map	textures/shaderlab_wall/cement_drk.png
	}
	{
		stage	specularmap
		map	textures/shaderlab_wall/cement_s.png
	}
	{
		stage	lightmap
	}
}

