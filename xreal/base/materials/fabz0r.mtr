textures/fabz0r/portal/round
{
	qer_editorimage textures/otty/sfx/portal_base.tga
	
	noimpact
	nomarks
	trans
	
	cull disable
	sort postProcess
 
	{
		blend blend
		map textures/otty/sfx/portal_base.tga
		clamp
   		rotate -40 * time
	}

	sort postProcess
	{
		stage	heathazemap
		map		textures/fabz0r/portal/round_normal.png
		clamp
		deformMagnitude 10.0
  		rotate 40 * time
		//centerScale 0.9, 0.9
    	blend blend
   	}
}// texture and material by Fabz0r

