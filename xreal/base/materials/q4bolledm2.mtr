textures/q4bolledm2/red
{
	noShadows
	nonsolid
	noimpact
	translucent
	deform	flare	8
	qer_editorimage	textures/editor/qer_flare.tga

	{
		blend 		add
		map 		_quadratic

		red			1
		green		0
		blue		0
		alpha		0.5
	}
}

textures/q4bolledm2/red_pulse
{
	noShadows
	nonsolid
	noimpact
	translucent
	deform	flare	8
	qer_editorimage	textures/editor/qer_flare.tga

	{
		blend 		add
		map 		_quadratic

	
		red			0.8 * parm0 * (1.2 + sintable [time * 0.75] )
		green		0
		blue		0
		alpha		0.5
