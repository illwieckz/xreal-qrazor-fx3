textures/q4bolledm2/doom3world
{
	noShadows
	noimpact
	nonsolid
	qer_editorimage textures/q4bolledm2/doom3world
	{
		blend		add
		map			textures/q4bolledm2/doom3world
		red		MCC_flicker1[ time * 1 ] * MCC_flicker2[ time * 1.75 ] * Parm0
		green		MCC_flicker1[ time * 1 ] * MCC_flicker2[ time * 1.75 ] * Parm1
		blue		MCC_flicker1[ time * 1 ] * MCC_flicker2[ time * 1.75 ] * Parm2
		//colored
	}
}

