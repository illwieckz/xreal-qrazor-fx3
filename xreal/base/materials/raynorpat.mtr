//
// Misc. (skies, dlights, etc...)
//

textures/raynorpat/black_sky
{
	qer_editorimage textures/raynorpat/black.png

	xmap_sun 1 1 1 32	90 90
	
	noimpact
	nodamage
	sky

	skyparms - 512 -

	{
		map textures/raynorpat/black.png
	}
}

//
// Effects
//
textures/raynorpat/effects/firewall
{
	qer_editorimage textures/raynorpat/effects/firewalla.png

	noimpact

	{
		map textures/raynorpat/effects/firewalla.png
		blend gl_one, gl_zero
		tcMod turb .2 .2 .1 1
		tcMod scale .25 .25
		tcMod scroll .5 1.5
	}
	{
		map textures/raynorpat/effects/firewallb.png
		blend gl_dst_color, gl_src_alpha
		tcMod turb .2 .1 .1 1
		tcMod scale .1 .1
		tcMod scroll 0 1.0
	}
	{
		map textures/raynorpat/effects/firewallc.png
		blend gl_dst_color, gl_one
		tcMod turb .1 .1 .5 .5
		tcMod scroll 1 5.0
	}
}

textures/raynorpat/effects/blue_firewall
{
	qer_editorimage textures/raynorpat/effects/blue_firewalla.png

	noimpact

	{
		map textures/raynorpat/effects/blue_firewalla.png
		blend gl_one, gl_zero
		tcMod turb .2 .2 .1 1
		tcMod scale .25 .25
		tcMod scroll .5 1.5
	}
	{
		map textures/raynorpat/effects/blue_firewallb.png
		blend gl_dst_color, gl_src_alpha
		tcMod turb .2 .1 .1 1
		tcMod scale .1 .1
		tcMod scroll 0 1.0
	}
	{
		map textures/raynorpat/effects/blue_firewallc.png
		blend gl_dst_color, gl_one
		tcMod turb .1 .1 .5 .5
		tcMod scroll 1 5.0
	}
}

