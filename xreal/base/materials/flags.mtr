

models/flags/flagpole
{
	twoSided
	diffuseMap		 models/flags/mic-flags/flagpole_diff
	normalMap		 models/flags/mic-flags/flagpole_NRM
	specularMap	 	 models/flags/mic-flags/flagpole_diff
}
models/flags/topding
{
	twoSided
	// deformVertexes wave 100 sin 5 5 .5 2	
	normalMap		models/flags/mic-flags/topding_NRM
	specularMap		models/flags/mic-flags/topding_diff

	{
	blend diffusemap
	map     models/flags/mic-flags/topding_diff
	alphatest .5
	}
}

