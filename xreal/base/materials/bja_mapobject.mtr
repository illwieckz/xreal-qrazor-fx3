models/mapobjects/bja/bja_light01
{
	qer_editorimage		models/mapobjects/bja/bja_light01_d.tga
	xmap_surfaceLight	3000
	xmap_lightImage		models/mapobjects/bja/bja_light01_add
	diffusemap		models/mapobjects/bja/bja_light01_d
	specularmap		models/mapobjects/bja/bja_light01_s
	bumpmap			addnormals ( models/mapobjects/bja/bja_light01_local.tga, heightmap (models/mapobjects/bja/bja_light01_h.tga, 2) )
	{ 
		blend		add
		map 		models/mapobjects/bja/bja_light01_add
		colored
	}
}

