
textures/reflex_special/wallwalk01_blue
{
    qer_editorimage  textures/reflex_special/wallwalk01.tga
    
    xmap_surfaceLight 800
	xmap_lightRGB 0.24 0.41 0.77
	xmap_lightImage textures/reflex_special/wallwalk01_glow.tga
    
    wallwalk
    
    diffusemap       textures/reflex_special/wallwalk01.tga
    bumpmap          textures/reflex_special/wallwalk01_local.tga
    specularMap      textures/reflex_special/wallwalk01_spec.tga
	{
		blend		add
		map	      textures/reflex_special/wallwalk01_glow
        color       0.24 * (jitter5 [time * 2]), 0.41 * (jitter5 [time * 2]), 0.77 * (jitter5 [time * 2]), 1.0
	}
}

