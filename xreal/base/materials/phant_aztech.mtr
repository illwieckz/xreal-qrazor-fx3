//------------------------ Jump Pad

textures/phant_aztech/aztech_pad_glow
{
	translucent
	nonSolid

//	{ 
//		maskColor
//		map makealpha(models/mapobjects/multiplayer/jump_pad/jump_pad_mask.tga)
//		centerScale .73, .73
//	}

	{
		blend GL_DST_ALPHA, gl_one
		maskAlpha
		map textures/phant_aztech/aztech_pad_glow.tga
		centerScale jumppadtable [time * 1], jumppadtable [time * 1]
		clamp
	}	

	
}

// ------------------------ Lava

textures/phant_aztech/aztech_lava_004
{
	qer_editorimage		textures/phant_aztech/aztech_lava_004.tga
	translucent
	nonsolid
	twosided

	{
		map     textures/phant_aztech/aztech_lava_004.tga
		scale	1 , 1
		translate	time * 0.005, time * -0.01
		rgb		aztechlavatable[ time * .05 ]
	}
	{
		blend add
		map     textures/phant_aztech/aztech_lava_004.tga
		scale	1.4, 1.5
		translate	time * 0, time * -0.03
		rgb		aztechlavatable[ time * .04 ]
	}
	{
		blend add
		map     textures/phant_aztech/aztech_lava_004.tga
		scale	.8, 1.1
		translate	time * -0.001, time * -0.02
		rgb		aztechlavatable[ time * .1 ]
	}
}

