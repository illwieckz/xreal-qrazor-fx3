rocketThrust
{
	twoSided
	{
		if((time * 15) % 3 == 0)
		map models/projectiles/rocket/thrustc1.tga
		blend GL_ONE, GL_SRC_ALPHA
		rgbGen wave inversesawtooth 0 1 0 15 
	}
	{
		if((time * 15) % 3 == 0)
		map models/projectiles/rocket/thrustc1.tga
		blend GL_ONE, GL_SRC_ALPHA
		rgbGen wave sawtooth 0 1 0 15 
	}
	{
		if((time * 15) % 3 == 1)
		map models/projectiles/rocket/thrustc2.tga
		blend GL_ONE, GL_SRC_ALPHA
		rgbGen wave inversesawtooth 0 1 0 15 
	}
	{
		if((time * 15) % 3 == 1)
		map models/projectiles/rocket/thrustc3.tga
		blend GL_ONE, GL_SRC_ALPHA
		rgbGen wave sawtooth 0 1 0 15 
	}
	{
		if((time * 15) % 3 == 2)
		map models/projectiles/rocket/thrustc3.tga 
		blend GL_ONE, GL_SRC_ALPHA
		rgbGen wave inversesawtooth 0 1 0 15 
	}
	{
		if((time * 15) % 3 == 2)
		map models/projectiles/rocket/thrustc1.tga 
		blend GL_ONE, GL_SRC_ALPHA
		rgbGen wave sawtooth 0 1 0 15 
	}
}

rocketFlare
{
	twoSided
	deformVertexes autosprite

	{
		map models/projectiles/rocket/rocketflare.tga
		blendfunc add
	}
}
