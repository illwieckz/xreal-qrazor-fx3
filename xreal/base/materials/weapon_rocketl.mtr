// missile

models/projectiles/missile/missile
{
	{
		stage diffuseMap
		map	models/projectiles/missile/missile.png
	}
	normalMap heightmap(models/projectiles/missile/missile_bump.png, 3)
	specularMap models/projectiles/missile/missile_spec.png
	{
		map models/projectiles/missile/missile_glow.png
		blend add
	}
}

models/projectiles/missile/missileflar
{
	twoSided
	{
		map models/projectiles/missile/missileflar.png
		blend add
		scroll time * 10.0, 0
	}
}

