
models/weapons/rocketl/rocketl
{
	{
		blend diffuseMap
		map models/weapons/rocketl/rocketl.tga
		rgbGen lightingDiffuse
	}
	//bumpMap heightMap(models/weapons/rocketl/rocketl_bump.tga, 3)
	bumpMap models/weapons/rocketl/rocketl_norm.tga
	specularMap models/weapons/rocketl/rocketl_spec.tga
}


models/weapons2/rocketl/f_rocketl
{
	twoSided
	sort additive
	{
		map models/weapons2/rocketl/f_rocketl.tga
		blend add
	}
}


models/projectiles/missile/missile
{
	{
		stage diffuseMap
		map	models/projectiles/missile/missile.tga
		rgbGen lightingDiffuse
	}
	bumpMap heightmap(models/projectiles/missile/missile_bump.tga, 3)
	specularMap models/projectiles/missile/missile_spec.tga
	{
		map models/projectiles/missile/missile_glow.tga
		blend add
	}
}

