
models/weapons/rocketl/rocketl
{
	{
		stage diffuseMap
		map models/weapons/rocketl/rocketl.tga
		rgbGen lightingDiffuse
	}
	//normalMap heightMap(models/weapons/rocketl/rocketl_bump.tga, 3)
	normalMap models/weapons/rocketl/rocketl_local.tga
	specularMap models/weapons/rocketl/rocketl_s.tga
}

models/weapons/rocketl/v_rocketl
{
	{
		stage diffuseMap
		map models/weapons/rocketl/v_rocketl.tga
		rgbGen lightingDiffuse
	}
	normalMap models/weapons/rocketl/v_rocketl_local.tga
	specularMap models/weapons/rocketl/v_rocketl_s.tga
}


models/weapons2/rocketl/f_rocketl
{
	twoSided
	sort additive
	{
		map models/weapons2/rocketl/f_rocketl.tga
		blend add
	}
}

models/projectiles/missile/missile
{
	{
		stage diffuseMap
		map	models/projectiles/missile/missile.tga
		rgbGen lightingDiffuse
	}
	normalMap heightmap(models/projectiles/missile/missile_h.tga, 3)
	specularMap models/projectiles/missile/missile_s.tga
	{
		map models/projectiles/missile/missile_glow.tga
		blend add
	}
}

models/projectiles/missile/missileflar
{
	twoSided
	{
		map models/projectiles/missile/missileflar.tga
		blend add
		scroll time * 10.0, 0
	}
}

models/projectiles/missile/missilefls1
{
	twoSided
	{
		map models/projectiles/missile/missilefls1.tga
		blend add
	}
}

models/projectiles/missile/missilefls2
{
	twoSided
	{
		map models/projectiles/missile/missilefls2.tga
		blend add
	}
}

models/ammo/rocket/rocket
{
	{
		stage diffuseMap
		map models/projectiles/rocket/rocket.tga
	}
	normalMap heightMap(models/projectiles/rocket/rocket.tga, 1)
	specularMap models/projectiles/rocket/rocket.tga
}

