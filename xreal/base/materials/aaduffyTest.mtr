particles/flowingblood
{
	noSelfShadow
	//noShadows
	//twosided
	//translucent
	{
			blend bumpmap	
			map textures/hell/lava1_local.tga
			centerscale	1+.05*sinTable[time * .1 ], 1+.05*sinTable[time * .1]
			//scroll	time * 0 , time * -0.04	
			vertexcolor
			
	}
	{
			blend diffusemap	
			map models/mapobjects/teleporter/telecolon1.tga
			//scale 1 , 3
			scroll	time * 0 , time * -0.06	
			alphaTest 0.5
			vertexcolor
			
	}

	{
			blend specularmap
			map models/mapobjects/teleporter/telecolon1_s.tga

			//scale 1 , 3
			scroll	time * 0 , time * -0.01	
			vertexcolor
			
	}
}


